image {
  width: 750rpx;
  height: 300rpx;
}

.modList {
  border-radius: 10px;
  flex-direction: column;
  border: 1px solid #ccc;
  box-sizing: border-box;
  Margin: 10px;
}

.dellList {
  border-radius: 10px;
  flex-direction: column;
  border: 1px solid #ccc;
  box-sizing: border-box;
  Margin: 10px 50px 10px -50px;
}

.wrapper {
  display: flex;
  flex-wrap: nowrap;
}

.circle {
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-around;
  padding: 20rpx 0rpx;
}

.item {
  font-size: 14px;
  box-sizing: border-box;
  height: 50px;
  border: 0px solid #ccc;
  width: 200px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.item2 {
  box-sizing: border-box;
  height: 50px;
  border: 0px solid #ccc;
  width: 200px;
  padding: 10px;
}

.subject {
  font-size: 14px;
  box-sizing: border-box;
  align-items: center;
}

.message {
  font-size: 12px;
  color: #999;
  box-sizing: border-box;
  padding: 5px 0px;
}

.page_imgs {
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-around;
}

.dell {
  float: right;
  /*background-color: orangered*/
}

.hint {
  padding: 5px;
  font-size: 12px;
  color: rosybrown;
  text-align: center;
}
